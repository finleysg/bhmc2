{
  "name": "BHMC",
  "version": "1.2.1",
  "description": "The Bunker Hills Men's Club website for event registration and results",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "less": "lessc ./src/assets/less/app.less ./src/assets/css/app.css",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "minify-css": "cleancss --source-map -o ./tmp/app.min.css ./src/assets/css/app.css",
    "ngc": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup.config.js",
    "build": "npm run ngc && npm run rollup && npm run less && node build",
    "build-patch": "npm version patch && npm run build",
    "build-minor": "npm version minor && npm run build",
    "publish": "cd ./dist; surge --domain https://oceanic-tendency.surge.sh/;"
  },
  "keywords": [
    "golf",
    "club",
    "registration"
  ],
  "author": "Stuart Finley (finleysg@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/compiler": "2.4.9",
    "@angular/compiler-cli": "2.4.9",
    "@angular/core": "2.4.9",
    "@angular/forms": "2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/platform-server": "2.4.9",
    "@angular/router": "3.4.9",
    "angular2-toaster": "2.0.0",
    "core-js": "2.4.1",
    "lodash": "4.17.4",
    "material-design-iconic-font": "2.2.0",
    "ng2-bootstrap": "1.4.2",
    "ng2-cookies": "1.0.8",
    "ng2-page-scroll": "4.0.0-beta.6",
    "ng2-slim-loading-bar": "2.4.0",
    "raven-js": "3.9.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.2.0",
    "showdown": "1.6.4",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.36",
    "@types/lodash": "4.14.58",
    "@types/moment": "2.13.0",
    "@types/node": "6.0.46",
    "@types/selenium-webdriver": "2.53.33",
    "@types/showdown": "1.4.32",
    "@types/stripe": "0.0.32",
    "canonical-path": "0.0.2",
    "clean-css-cli": "4.0.0",
    "concurrently": "3.1.0",
    "fs-extra": "2.0.0",
    "http-server": "0.9.0",
    "jasmine-core": "2.4.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "less": "2.7.1",
    "lite-server": "2.2.2",
    "protractor": "4.0.13",
    "replacestream": "4.0.2",
    "rimraf": "2.5.4",
    "rollup": "0.41.6",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-uglify": "1.0.1",
    "systemjs": "0.19.47",
    "tslint": "3.15.1",
    "typescript": "2.2.2"
  },
  "repository": {}
}
